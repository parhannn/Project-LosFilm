<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/SplashBasedStyle"
    tools:context=".Splash">

    <ImageView
        android:id="@+id/apk_logo"
        android:contentDescription="@string/apk_logo"
        style="@style/LogoStyle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/apk_logo" />

    <ImageView
        android:id="@+id/team_logo"
        android:contentDescription="@string/team_logo"
        style="@style/MadeByStyle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/apk_logo"
        app:layout_constraintVertical_bias="0.852"
        app:srcCompat="@drawable/made_by" />

</androidx.constraintlayout.widget.ConstraintLayout>